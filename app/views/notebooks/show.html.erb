<div class="header">
  <div class="container">
    <img src="" >
    <h1>NotesApp</h1>
  </div>
</div>

 <div class="tag">
  <div class="container">

    <div class="notes">
      <div class="container">

        <% @note.each do |notes| %>
            <div class="note">
              <p class="content"><%= notes.content %></p>

              <!--<p class="time"><%= notes.created_at %></p> -->
            </div>
        <% end %>


        <%= link_to "New Note", new_notebook_path(@notebook) %>

  </div>
</div>
